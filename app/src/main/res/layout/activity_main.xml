<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/sim"
    android:orientation="vertical"
    tools:context=".MainActivity"
    tools:ignore="ExtraText,UseCompoundDrawables">


    <Button
        android:id="@+id/button3"
        android:layout_width="201dp"
        android:layout_height="67dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="6dp"
        android:layout_marginBottom="13dp"
        android:layout_weight="1"
        android:onClick="abrir_login"
        android:text="Login"
        android:textAllCaps="true"
        android:textSize="23sp"
        tools:ignore="ButtonStyle,HardcodedText,MissingConstraints,UsingOnClickInXml,VisualLintBounds,VisualLintButtonSize" />

    <Button
        android:id="@+id/btnNavigate"
        android:layout_width="wrap_content"
        android:layout_height="67dp"
        android:layout_alignEnd="@+id/button3"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="-202dp"
        android:layout_marginBottom="13dp"
        android:layout_weight="1"
        android:onClick="abrir_tela_sobre"
        android:text="Cadastrar"
        android:textAllCaps="true"
        android:textSize="23sp"
        android:visibility="visible"
        tools:ignore="ButtonStyle,DuplicateSpeakableTextCheck,HardcodedText,MissingConstraints,UsingOnClickInXml,VisualLintBounds,VisualLintButtonSize" />


</RelativeLayout>